{
  "name": "Poweradmin",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [
    "app:9000",
    "mysql:3306",
    "pgsql:5432",
    "adminer:8090",
    "dns:1053",
    "dns:8081"
  ],
  "portsAttributes": {
    "app:9000": {
      "label": "Poweradmin"
    },
    "mysql:3306": {
      "label": "MariaDB"
    },
    "pgsql:5432": {
      "label": "PostgreSQL"
    },
    "adminer:8090": {
      "label": "Adminer"
    },
    "dns:1053": {
      "label": "PowerDNS"
    },
    "dns:8081": {
      "label": "PowerDNS API"
    }
  }
}
